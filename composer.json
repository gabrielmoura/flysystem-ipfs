{
    "name": "gabrielmoura/flysystem-ipfs",
    "description": "Flysystem adapter for the ipfs",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Moura",
            "email": "gabriel.all@yandex.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "league/flysystem": "1.*",
        "cloutier/php-ipfs-api": "0.0.6"
